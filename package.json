{
  "name": "brisbane-city-council-free-native-plants",
  "description": "Council’s Free Native Plants program offers a range of plants to assist you to plant and green your property. The native species provided through the program help grow our city’s urban forest and support local wildlife.",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build --modern",
    "build:analyze": "npm run build -- --analyze",
    "bundlewatch": "bundlewatch --config bundlewatch.config.json",
    "start": "nuxt-ts start",
    "serve:lhci": "npm run start",
    "serve:test": "cross-env PORT=3001 npm run start",
    "lhci:autorun": "lhci autorun",
    "generate": "nuxt-ts generate --modern",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:style -- --fix",
    "images": "node images.js",
    "test": "jest",
    "test:e2e": "codeceptjs run --steps"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "1.8.1",
    "@nuxt/typescript-runtime": "1.0.0",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/markdownit": "1.2.10",
    "@nuxtjs/pwa": "3.0.2",
    "@nuxtjs/sitemap": "2.4.0",
    "dayjs": "1.8.35",
    "github-markdown-css": "4.0.0",
    "lodash-es": "4.17.15",
    "mapbox-gl": "1.12.0",
    "nuxt": "2.14.4",
    "store": "2.0.12",
    "vue-warehouse": "2.2.0"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "1.3.0",
    "@lhci/cli": "0.5.0",
    "@mdi/js": "5.5.55",
    "@nuxt/types": "2.14.4",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.11.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@vue/test-utils": "1.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "bundlewatch": "0.2.7",
    "codeceptjs": "2.6.10",
    "codeceptjs-resemblehelper": "1.9.2",
    "cross-env": "7.0.2",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.0",
    "imagemin-svgo": "8.0.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "lqip-loader": "2.2.0",
    "playwright": "1.4.0",
    "prettier": "2.1.1",
    "puppeteer": "5.3.0",
    "responsive-loader": "2.1.1",
    "sharp": "0.26.0",
    "sqip-loader": "1.0.0",
    "stylelint": "13.7.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "vue-jest": "3.0.6",
    "webp-loader": "0.6.0"
  }
